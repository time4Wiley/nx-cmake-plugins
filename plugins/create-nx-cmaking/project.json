{
    "name": "create-nx-cmaking",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "plugins/create-nx-cmaking/bin",
    "projectType": "library",
    "targets": {
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "plugins/create-nx-cmaking/**/*.ts",
                    "plugins/create-nx-cmaking/package.json"
                ]
            },
            "configurations": {
                "ci": {
                    "maxWarnings": 0
                }
            }
        },
        "build": {
            "dependsOn": ["lint", "^build"],
            "executor": "@nx/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/plugins/create-nx-cmaking",
                "main": "plugins/create-nx-cmaking/bin/index.ts",
                "tsConfig": "plugins/create-nx-cmaking/tsconfig.lib.json",
                "assets": ["plugins/create-nx-cmaking/*.md"]
            },
            "configurations": {
                "ci": {}
            }
        },
        "test": {
            "dependsOn": ["build"],
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "plugins/create-nx-cmaking/jest.config.ts"
            },
            "configurations": {
                "ci": {
                    "coverageReporters": ["lcov"]
                }
            }
        },
        "publish": {
            "dependsOn": ["build"],
            "command": "nx upload verdaccio --name={projectName} --ver=0.0.0 --tag=latest"
        }
    },
    "tags": [],
    "implicitDependencies": ["nx-cmaking"]
}
